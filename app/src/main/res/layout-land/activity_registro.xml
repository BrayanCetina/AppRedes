<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bag4"
    tools:context=".registro">

  <ImageView
      android:id="@+id/imageView"
      android:layout_width="229dp"
      android:layout_height="0dp"
      android:layout_marginTop="5dp"
      android:layout_marginBottom="10dp"
      app:layout_constraintBottom_toTopOf="@+id/txtPass1"
      app:layout_constraintEnd_toEndOf="@+id/editText6"
      app:layout_constraintStart_toEndOf="@+id/editText6"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@mipmap/ic_launcher" />

  <TextView
      android:id="@+id/textView2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="12dp"
      android:layout_marginBottom="1dp"
      android:text="Nombres:"
      app:layout_constraintBottom_toBottomOf="@+id/editText6"
      app:layout_constraintEnd_toStartOf="@+id/editText6" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="51dp"
      android:layout_marginBottom="116dp"
      android:text="Apellidos:"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="28dp"
      android:text="Correo:"
      app:layout_constraintBottom_toBottomOf="@+id/txtUser"
      app:layout_constraintEnd_toStartOf="@+id/txtUser" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="1dp"
      android:text="Contraseña:"
      app:layout_constraintBottom_toBottomOf="@+id/txtPass1"
      app:layout_constraintEnd_toStartOf="@+id/txtPass1" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Contraseña:"
      app:layout_constraintBottom_toBottomOf="@+id/txtPass2"
      app:layout_constraintEnd_toStartOf="@+id/txtPass2" />

  <EditText
      android:id="@+id/txtUser"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="12dp"
      android:layout_marginTop="9dp"
      android:ems="10"
      android:inputType="textEmailAddress"
      app:layout_constraintStart_toEndOf="@+id/textView3"
      app:layout_constraintTop_toBottomOf="@+id/textView3" />

  <EditText
      android:id="@+id/txtPass1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="27dp"
      android:ems="10"
      android:inputType="textPassword"
      app:layout_constraintBottom_toBottomOf="@+id/editText6"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="@+id/editText6" />

  <EditText
      android:id="@+id/txtPass2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="9dp"
      android:ems="10"
      android:inputType="textPassword"
      app:layout_constraintStart_toStartOf="@+id/txtPass1"
      app:layout_constraintTop_toBottomOf="@+id/txtPass1" />

  <EditText
      android:id="@+id/editText6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="9dp"
      android:ems="10"
      android:inputType="textPersonName"
      app:layout_constraintBottom_toTopOf="@+id/txtFolio"
      app:layout_constraintStart_toStartOf="@+id/txtFolio" />

  <EditText
      android:id="@+id/txtFolio"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="9dp"
      android:ems="10"
      android:inputType="textPersonName"
      app:layout_constraintBottom_toTopOf="@+id/txtUser"
      app:layout_constraintStart_toStartOf="@+id/txtUser" />

  <Button
      android:id="@+id/button1"
      android:layout_width="115dp"
      android:layout_height="47dp"
      android:layout_marginStart="14dp"
      android:layout_marginTop="56dp"
      android:layout_marginBottom="58dp"
      android:text="Aceptar"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="@+id/textView6"
      app:layout_constraintTop_toTopOf="@+id/txtPass2" />

  <Button
      android:id="@+id/button2"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginStart="34dp"
      android:layout_marginTop="9dp"
      android:layout_marginEnd="39dp"
      android:layout_marginBottom="2dp"
      android:onClick="Main"
      android:text="Cancelar"
      app:layout_constraintBottom_toBottomOf="@+id/button1"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/button1"
      app:layout_constraintTop_toBottomOf="@+id/txtPass2" />

</androidx.constraintlayout.widget.ConstraintLayout>